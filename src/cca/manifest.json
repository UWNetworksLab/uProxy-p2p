{
  "manifest_version": 2,
  "name": "uProxy",
  "description": "Get and share access to the open internet",
  "minimum_chrome_version": "41.0.2272.63",
  "version": "0.0.8",
  "icons": {
      "36": "assets/icons/launcher-ldpi.png",
      "48": "assets/icons/launcher-mdpi.png",
      "72": "assets/icons/launcher-hdpi.png",
      "96": "assets/icons/launcher-xhdpi.png"
  },
  "app": {
    "background": {
      "scripts": [
        "scripts/background.static.js"
      ]
    }
  },
  "permissions": [
    "identity",
    "storage",
    "http://*/",
    "https://*/",
    "notifications",
    "proxy",
    "system.network"
  ],
  "sockets": {
    "tcp": { "connect" : "" },
    "tcpServer": { "listen": "" },
    "udp": { "send": "", "bind": "" }
  },
  // This is essentially the default CSP that would have been generated by
  // cca-manifest-logic/src/analyse-manifest.js, but with "img-src data: *" on
  // the end so that we can render profile pictures from the social providers.
  "csp": "default-src file: data: blob: chrome-extension: gopher: gap: https://ssl.gstatic.com; connect-src *; media-src *;  style-src file: data: blob: chrome-extension: gopher: gap: https://ssl.gstatic.com 'unsafe-inline'; img-src data: *;",
  "externally_connectable": {
    "matches" : ["*://*.uproxy.org/*", "*://uproxysite.appspot.com/*", "*://test-dot-uproxysite.appspot.com/*", "*://beta-dot-uproxysite.appspot.com/*"]
  },
  "minSdkVersion": 16,  // Limited by Crosswalk 22
  "key": ""
}
