{
  "manifest_version": 2,
  "name": "UProxyApp",
  "description": "Ubiquitous Proxy [Alpha]",
  "minimum_chrome_version": "34",
  "version": "0.1.8",
  "icons": {
      "128": "icons/uproxy-128.png",
      "19": "icons/uproxy-19.png",
      "16": "icons/uproxy-16.png"
  },
  "app": {
    "background": {
      "page": "uproxy.html"
    }
  },
  "permissions": [
    {"socket": ["tcp-connect", "udp-send-to", "tcp-listen", "udp-bind"]},
    "storage",
    "webview",
    "identity",
    "videoCapture",
    "audioCapture",
    "https://apis.google.com/",
    "https://*.appspot.com/",
    "https://www.facebook.com/",
    "https://graph.facebook.com/",
    "https://www.googleapis.com/*",
    "https://accounts.google.com/*"
  ],
   "key": ""
}
