{
  "manifest_version": 2,
  "name": "UProxyApp",
  "description": "Ubiquitous Proxy [Alpha]",
  "minimum_chrome_version": "38",
  "version": "0.3.0",
  "icons": {
      "128": "icons/uproxy-128.png",
      "19": "icons/uproxy-19.png",
      "16": "icons/uproxy-16.png"
  },
  "app": {
    "background": {
      "page": "uproxy.html"
    }
  },
  "permissions": [
    "storage",
    "webview",
    "videoCapture",
    "audioCapture",
    "https://graph.facebook.com/oauth/access_token",
    "https://accounts.google.com/o/oauth2/token"
  ],
  "sockets": {
    "tcp": { "connect" : "" },
    "tcpServer": { "listen": "" },
    "udp": { "send": "", "bind": "" }
  },
   "key": ""
}
