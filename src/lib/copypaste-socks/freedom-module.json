{
  "name": "copypaste-socks",
  "description": "SOCKS server with copy/paste-based signalling channel.",
  "app": {
    "script": [
      "freedom-module.static.js"
    ]
  },
  "dependencies": {
    "loggingcontroller": {
      "url": "../loggingprovider/freedom-module.json",
      "api": "loggingcontroller"
    },
    "logginglistener": {
      "url": "../loggingprovider/freedom-module.json",
      "api": "logginglistener"
    },
    "churnPipe": {
      "url": "../churn-pipe/freedom-module.json",
      "api": "churnPipe"
    },
    "pgp": {
      "url": "../../freedom-pgp-e2e/dist/pgpapi.json",
      "api": "crypto"
    },
    "portControl": {
      "url": "../../freedom-port-control/dist/port-control.json",
      "api": "portControl"
    }
  },
  "permissions": [
    "core.console",
    "core.crypto",
    "core.storage",
    "core.tcpsocket",
    "core.udpsocket",
    "core.rtcdatachannel",
    "core.rtcpeerconnection"
  ]
}
