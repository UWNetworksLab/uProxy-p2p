{
  "name": "UProxy",
  "description": "The UProxy backend provides a mesh web proxy, access the web through your social connections.",
  "app": {
    "script": [
      "uproxy.js",
      "view_oauth.js",
      "util.js",
      "nouns-and-adjectives.js",
      "constants.js",
      "state-storage.js",
      "social.js",
      "core.js",
      "start-uproxy.js"
    ]
  },
  "dependencies": {
    "SocksToRtc": {
      "url": "../lib/socks-to-rtc/socks-to-rtc.json"
    },
    "RtcToNet": {
      "url": "../lib/rtc-to-net/rtc-to-net.json"
    },
    "SOCIAL-websocket": {
      "url": "../lib/websocket-server/social.ws.json",
      "api": "social"
    },
    "storage": {
      "url": "../lib/storage/storage.isolated.json",
      "api": "storage"
    }
  },
  "permissions": [
    "core.peerconnection"
  ]
}
